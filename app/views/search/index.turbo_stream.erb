  <% @clienti.each do |cliente| %>
      <%= turbo_stream.append("itemsList") do %>
        <%= render CommandMenu::ItemComponent.new(remote: true, target: cliente, title: "#{cliente.denominazione} - #{cliente.comune}") %>
      <% end %>
  <% end %>
<% end %>

<% if !@libri.empty? %>
  <% @libri.each do |libro| %>
      <%= turbo_stream.append("itemsList") do %>
        <%= render CommandMenu::ItemComponent.new(remote: true, target: libro, title: "#{libro.titolo} - #{libro.editore.editore}") %>
      <% end %>
  <% end %>
<% end %>

