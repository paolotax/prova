
<% if !@import_scuole.empty? %>
  <% @import_scuole.each do |scuola| %>
      <%= turbo_stream.append("itemsList") do %>
        <%= render CommandMenu::ItemComponent.new(remote: true, target: scuola, title: "#{scuola.denominazione} - #{scuola.citta}") %>
      <% end %>
  <% end %>
<% end %>

