<% @selected_tappe.each do |tappa| %>
    <%= turbo_stream.replace "tappa_#{tappa.id}", partial: "tappa", locals: { tappa: tappa } %>
<% end %>

<% if @giro %>
    <%= turbo_stream.replace "giro_tappe_filter_#{dom_id @giro }" do %>
        <%= render partial: "giri/giro_filter", locals: { giro: @giro } %>
    <% end %>
<% end %>