<% @import_scuole.map do |import_scuola| %>
    <% unless import_scuola.id.nil? %>
        <turbo-stream action="update" targets=".tappe-import_scuola-<%= import_scuola.id %>"> 
            <template>
                <%= render partial: "clientables/lista_tappe", formats: :html, locals: { tappe: import_scuola.tappe } %>
            </template>
        </turbo-stream>  
    <% end %>
<% end %>        