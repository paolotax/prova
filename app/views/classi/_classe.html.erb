<%= tag.div dom_id(classe), class: "inline-block max-w-24 p-1 bg-gray-100 border rounded-md m-1" do %>


    <%= tag.div class: "flex flex-row text-md font-medium text-gray-900 items-baseline" do %>
        <%= tag.div class: "" do %>
            <%= classe.classe %>
        <% end %>
        <%= tag.div class: "" do %>
            <%= classe.sezione %>
        <% end %>
        <%= tag.div class: "pl-2 text-xs text-gray-500 truncate" do %>
            <%= classe.combinazione.downcase %>
        <% end %>
    <% end %>


<% end %>
