<% @appunti.map do |appunto| %>
    <% unless appunto.import_scuola_id.nil? %>
        <turbo-stream action="update" targets=".tappe-import_scuola-<%= appunto.import_scuola_id %>"> 
            <template>
                <%= render partial: "clientable/lista_tappe", formats: :html, locals: { tappe: appunto.import_scuola.tappe } %>
            </template>
        </turbo-stream>  
    <% end %>
<% end %>           
